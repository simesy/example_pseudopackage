{
    "name": "simesy/example_pseudopackage",
    "description": "A way to bundle modules together that can be used by sites and distributions",
    "type": "drupal-module",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0+",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/core": "^8.4.2",
        "drupal/jsonapi": "^1.3",
        "drupal/jsonapi_extras": "^1.0@beta",
        "drupal/subrequests": "^2.0@beta",
        "drupal/fieldable_path": "^1.0",
        "drupal/simple_oauth": "^3.3",
        "drupal/remote_stream_wrapper": "1.0",
        "drupal/schemata": "^1.0@alpha",
        "drupal/openapi": "^1.0@alpha",
        "drupal/consumers": "^1.0@beta",
        "drupal/consumer_image_styles": "^2.0@rc",
        "drupal/graphql": "^3.0@beta",
        "drupal/decoupled_router": "^1.0@beta"
    },
    "require-dev": {
        "drupal/coder": "^8.2",
        "phpunit/phpunit": "~4.8"
    }
}
